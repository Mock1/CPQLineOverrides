<?xml version="1.0" encoding="UTF-8"?>
<CustomPermission xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Allows users to use the Delete button/link on Opportunity Line or Quote Line records. Without this permission, deletion can only occur in the CPQ Quote Line Editor.</description>
    <label>Delete Opportunity/Quote Lines</label>
    <requiredPermission>
        <customPermission>EditCPQLines</customPermission>
        <dependency>true</dependency>
    </requiredPermission>
</CustomPermission>
